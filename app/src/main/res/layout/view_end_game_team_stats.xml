<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android">

    <TextView
        android:id="@+id/team_winner"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="W or L"
        android:gravity="center_horizontal"/>

    <TextView
        android:id="@+id/text_mvp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:text="MVP"
        android:gravity="center_horizontal"/>

    <ImageView
        android:id="@+id/team_mvp"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_gravity="center_horizontal"/>

    <TextView
        android:id="@+id/team_kda"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="K/D/A"
        android:gravity="center_horizontal"/>

    <TextView
        android:id="@+id/team_gold"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="000g"
        android:gravity="center_horizontal"/>

    <TextView
        android:id="@+id/team_tower"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Tower:1"
        android:gravity="center_horizontal"/>

    <TextView
        android:id="@+id/team_dragon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Dragon:1"
        android:gravity="center_horizontal"/>

    <TextView
        android:id="@+id/team_baron"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Baron:1"
        android:gravity="center_horizontal"/>

    <TextView
        android:id="@+id/team_damage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TotalDamageToChamps:1"
        android:gravity="center_horizontal"/>

    <TextView
        android:id="@+id/team_ward"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="WardPlaced:1"
        android:gravity="center_horizontal"/>


</merge>